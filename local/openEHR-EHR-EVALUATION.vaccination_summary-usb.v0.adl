definition
	EVALUATION[at0000.1] matches {    -- Vaccination summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0052] occurrences matches {0..1} matches {    -- Vaccine name
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0071,    -- DT
									at0072,    -- dT
									at0073,    -- DTPa
									at0074,    -- dTpa
									at0075,    -- HBV
									at0076,    -- Hib
									at0077,    -- HPV
									at0078,    -- HZ
									at0079,    -- IPV
									at0080,    -- 4CMenB
									at0081,    -- MCV-C
									at0082,    -- MCV-ACWY
									at0083,    -- MMR
									at0084,    -- MMRV
									at0085,    -- PCV
									at0086,    -- PCV13
									at0087,    -- PPV23
									at0088,    -- VZV
									at0097,    -- FSME-Impfung
									at0102,    -- HAV
									at0103,    -- HAV und HBV
									at0104,    -- PrEP
									at0105,    -- PEP
									at0106,    -- Rotavirusimpfung
									at0112,    -- Grippeimpfung
									at0113]    -- Covid-19-Impfung
								}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..*} matches {    -- Targeted disease or agent
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0055,    -- Diphtherie
									at0056,    -- Tetanus
									at0057,    -- Pertussis
									at0058,    -- Haemophilus influenzae Typ b
									at0059,    -- Poliomyelitis
									at0060,    -- Hepatitis B
									at0061,    -- Pneumokokken
									at0062,    -- Masern
									at0063,    -- Mumps
									at0064,    -- Röteln
									at0065,    -- Varizellen
									at0066,    -- Humane Papillomaviren
									at0067,    -- Influenza
									at0068,    -- Meningokokken Serogruppe B
									at0069,    -- Meningokokken Serogruppe C
									at0070,    -- Meningokokken Serogruppe A, C, W und Y
									at0096,    -- Frühsommer-Meningoenzephalitis
									at0107,    -- Hepatitis A
									at0108,    -- Herpes Zoster
									at0109,    -- Tollwut
									at0110,    -- Rotaviren
									at0111,    -- Grippe
									at0114]    -- Covid-19-Erkrankung
								}
							}
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Primary course status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004,    -- Course not commenced
									at0005,    -- Course incomplete
									at0006,    -- Course complete
									at0007]    -- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Total number of administrations
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::at0048]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Date course completed
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Date of last vaccination
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Next vaccination due
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0051] occurrences matches {0..1} matches {    -- Additional details
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0022.1] occurrences matches {0..*} matches {    -- Per vaccine administration
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0023] occurrences matches {0..*} matches {    -- Specific vaccine name
								value matches {
									DV_CODED_TEXT matches {*}
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0034] occurrences matches {0..*} matches {    -- Vaccine details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0038] occurrences matches {0..1} matches {    -- Administration category
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0039,    -- Primary course
											at0040]    -- Booster
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {    -- Date of administration
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Sequence in series
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=1|}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0042] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- Administration location
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0045] occurrences matches {0..1} matches {    -- Administration outcome
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0046] occurrences matches {0..1} matches {    -- Information source
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Additional details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.dosage(-[a-zA-Z0-9_]+)*\.v2/}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {    -- Administration comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0.1] occurrences matches {0..1} matches {    -- Valid from
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Valid until
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Valid for
						value matches {
							DV_DURATION matches {*}
						}
					}
					ELEMENT[at0.4] occurrences matches {0..1} matches {    -- Life long validity
						value matches {
							DV_BOOLEAN matches {
								value matches {true}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0013] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0116] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {    -- Information source
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0050] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

