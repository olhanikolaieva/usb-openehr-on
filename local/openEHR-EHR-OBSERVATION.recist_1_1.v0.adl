				["at0101"] = <
					text = <"Best overall response">
					description = <"">
				>
				["at0102"] = <
					text = <"Complete response (CR)">
					description = <"">
				>
				["at0103"] = <
					text = <"Partial response (PR)">
					description = <"">
				>
				["at0104"] = <
