																units = <"mm">
															>
														>
													>
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {    -- Description of lesion
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0143] occurrences matches {0..1} matches {    -- Lymph node
