											}
											ELEMENT[at0174] occurrences matches {0..1} matches {    -- Time point status/response
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0175,    -- Present
															at0176,    -- Stable
															at0177,    -- Not evaluated
															at0178,    -- Not all evaluated
															at0179,    -- Unequivocal progression
